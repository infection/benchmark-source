{
    "name": "infection/benchmark-code-generator",
    "description": "Generator to create source code and tests.",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Th√©o FIDRY",
            "email": "theo.fidry@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "symfony/console": "^7.3",
        "symfony/filesystem": "^7.3",
        "symfony/finder": "^7.3",
        "symfony/process": "^7.3",
        "webmozart/assert": "^1.12"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.4",
        "rector/rector": "^2.2"
    },
    "bin": "bin/console",
    "autoload": {
        "psr-4": {
            "Infection\\BenchmarkCodeGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Infection\\BenchmarkCodeGenerator\\Test\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "infection/extension-installer": true,
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    }
}
