{
    "name": "infection/benchmark-source",
    "description": "Artificial codebase for benchmarking purposes.",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Th√©o FIDRY",
            "email": "theo.fidry@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0",
        "rector/rector": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "Infection\\BenchmarkSource\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Infection\\BenchmarkSource\\Test\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "infection/extension-installer": true,
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    }
}
